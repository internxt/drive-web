{
  "name": "drive-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mapbox/react-click-to-select": "^2.2.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.21.1",
    "bip39": "^3.0.3",
    "bootstrap": "^4.5.3",
    "check-password-strength": "^1.0.15",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "history": "^4.10.1",
    "immutability-helper": "^3.1.1",
    "jquery": "^3.5.1",
    "js-file-download": "^0.4.12",
    "natural-orderby": "^2.0.3",
    "openpgp": "^4.10.10",
    "popper.js": "^1.16.1",
    "prettysize": "^2.0.0",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-device-detect": "^1.15.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-timeago": "^5.2.0",
    "react-toastify": "^6.2.0",
    "reactjs-popup": "^2.0.4",
    "sanitize-filename": "^1.6.3",
    "socket.io-client": "^3.1.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "preinstall": "node ./scripts/use-yarn.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
    "lint:js": "eslint src/**/*.{js,ts,tsx,tsc}",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint src/**/*.{css,scss}",
    "lint:scss:fix": "npm run lint:scss -- --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "semi": "error",
      "comma-dangle": ["error", "never"],
      "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0, "maxBOF": 0 }],
      "eqeqeq": "error",
      "comma-spacing": ["error", { "before": false, "after": true }],
      "no-trailing-spaces": ["error"],
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "object-curly-spacing": ["error", "always"],
      "newline-after-var": ["error", "always"],
      "no-multi-spaces": "error",
      "curly": "error",
      "keyword-spacing": "error",
      "space-in-parens": ["error", "never"],
      "no-unused-vars": ["error"],
      "no-useless-constructor": ["error"],
      "no-extra-semi": ["error"]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.26.0",
    "@types/analytics-node": "^3.1.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/socket.io-client": "^1.4.35",
    "prop-types": "^15.7.2",
    "sass": "^1.32.4",
    "stylelint": "^13.4.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2"
  },
  "proxy": "http://localhost:8000"
}
